#ifndef INTRINSIC_FUNCTION
#define INTRINSIC_FUNCTION(NAME, VERSION)
#endif

//
// Version flags allowed:
//   FUNALL    - This is an intrinsic functions in all variants of Fortran.
//   FUNNOTF77 - This is an intrinsic functions  keyword for all variants of Fortran except 77.
//   FUNF77    - This is an intrinsic functions  keyword for Fortran 77.
//   FUNF90    - This is an intrinsic functions  keyword for Fortran 90.
//   FUNF95    - This is an intrinsic functions  keyword for Fortran 90/95.
//   FUNF2003  - This is an intrinsic functions  keyword for Fortran 2003.
//   FUNF2008  - This is an intrinsic functions  keyword for Fortran 2003/2008.
//

// conversion functions
INTRINSIC_FUNCTION(INT, FUNALL)
INTRINSIC_FUNCTION(REAL, FUNALL)
INTRINSIC_FUNCTION(DBLE, FUNALL)
INTRINSIC_FUNCTION(CMPLX, FUNALL)
INTRINSIC_FUNCTION(ICHAR, FUNALL)
INTRINSIC_FUNCTION(CHAR, FUNALL)

INTRINSIC_FUNCTION(AINT, FUNALL) // truncation of real
INTRINSIC_FUNCTION(ANINT, FUNALL) // nearest whole number
INTRINSIC_FUNCTION(NINT, FUNALL) // Nearest integer
INTRINSIC_FUNCTION(ABS, FUNALL) // Absolute value
INTRINSIC_FUNCTION(MOD, FUNALL) // Remainder
INTRINSIC_FUNCTION(SIGN, FUNALL) // Sign
INTRINSIC_FUNCTION(DIM, FUNALL) // Positive difference
INTRINSIC_FUNCTION(DPROD, FUNALL) // real * real => double prec
INTRINSIC_FUNCTION(MAX, FUNALL)
INTRINSIC_FUNCTION(MIN, FUNALL)
INTRINSIC_FUNCTION(LEN, FUNALL)
INTRINSIC_FUNCTION(INDEX, FUNALL) // location of substring a in b
INTRINSIC_FUNCTION(AIMAG, FUNALL) // imaginary part of complex
INTRINSIC_FUNCTION(CONJG, FUNALL) // conjugate of complex

// maths
INTRINSIC_FUNCTION(SQRT, FUNALL)
INTRINSIC_FUNCTION(EXP, FUNALL)
INTRINSIC_FUNCTION(LOG, FUNALL)
INTRINSIC_FUNCTION(LOG10, FUNALL)
INTRINSIC_FUNCTION(SIN, FUNALL)
INTRINSIC_FUNCTION(COS, FUNALL)
INTRINSIC_FUNCTION(TAN, FUNALL)
INTRINSIC_FUNCTION(ASIN, FUNALL)
INTRINSIC_FUNCTION(ACOS, FUNALL)
INTRINSIC_FUNCTION(ATAN, FUNALL)
INTRINSIC_FUNCTION(ATAN2, FUNALL)
INTRINSIC_FUNCTION(SINH, FUNALL)
INTRINSIC_FUNCTION(COSH, FUNALL)
INTRINSIC_FUNCTION(TANH, FUNALL)

// lexical comparison
INTRINSIC_FUNCTION(LGE, FUNALL)
INTRINSIC_FUNCTION(LGT, FUNALL)
INTRINSIC_FUNCTION(LLE, FUNALL)
INTRINSIC_FUNCTION(LLT, FUNALL)
